function [covariance_matrix] = get_covariance_matrix(vectors, number_of_vectors)
   
    covariance_matrix = zeros(number_of_vectors);
    for vector_1_index = 1:number_of_vectors
        for vector_2_index = 1:number_of_vectors
            covariance_matrix(vector_1_index, vector_2_index) = get_covariance(vectors(vector_1_index,:), vectors(vector_2_index,:));
        end
    end
end
